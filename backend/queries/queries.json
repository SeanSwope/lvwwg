  "duplicateLink" : "SELECT COUNT([ID]) AS COUNT FROM [dbo].[LINKDETAILS] WHERE [COMPANYNAME]=@name AND [CATEGORY]=@category",

  "getCategories" : "SELECT NEWID() AS ID,T1.* FROM (SELECT distinct [CATEGORY] FROM [dbo].[LINKDETAILS]) T1",

