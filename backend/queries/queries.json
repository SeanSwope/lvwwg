  "deleteMember" : "DELETE FROM [dbo].[MEMBERS] WHERE [ID]=@id",
  "updateMember" : "UPDATE [dbo].[MEMBERS] SET [FIRSTNAME]=@firstName,[LASTNAME]=@lastName,[EMAIL]=@email,[PASSWORD]=@password,[ADMIN]=@admin WHERE [ID]=@id",

